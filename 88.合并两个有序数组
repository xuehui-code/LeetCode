class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        for(int i=0;i<n;i++){
            nums1[m+i] = nums2[i];
        }
        int temp = 0;
        for(int i=1;i<n+m;i++){
            for(int j=0;j<i;j++){
                if(nums1[j]>=nums1[i]){
                    temp = nums1[j];
                    nums1[j] = nums1[i];
                    nums1[i] = temp;
                }
            }
        }
    }
}
